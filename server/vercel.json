{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "app.js"
      }
    ],
    "env": {
      "DB_USERNAME": "your_db_username",
      "DB_PASSWORD": "your_db_password"
    }
  }
  